{
  "expo": {
    "name": "Feature Starters",
    "slug": "feature-starters",
    "scheme": "featurestarters",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png"
        }
      ],
      [
        "react-native-maps",
        {
          "config": {
            "googleMaps": {
              "apiKey": "AIzaSyBDkzWmOPsrdRUzz5mo5DtpZim-t9HllOk"
            }
          }
        }
      ]
    ]
  }
}


